import argparse
subparse = argparse.ArgumentParser(description="CLI for finding and starting pipeline")
subparse.add_argument("grpId", help="It will contain groupID")
subparse.add_argument("artID", help="It will contain artifactID")
subparse.add_argument("--version", help="It will contain artifactID")

args = subparse.parse_args()

print(args.grpId)
print(args.artID)
print(args.version)
